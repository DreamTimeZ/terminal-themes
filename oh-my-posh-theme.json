{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {},
          "template": "x{{ reason .Code }}",
          "foreground": "#b8ff75",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "style": "roundrock"
          },
          "template": "{{ .FormattedMs }} ",
          "foreground": "#b8ff75",
          "type": "executiontime",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "1h",
            "mapped_shell_names": {
              "bash": "Bash ",
              "cmd": "Cmd ",
              "powershell": "WinPS ",
              "pwsh": "PS "
            }
          },
          "template": "{{ .Name }}",
          "foreground": "#E06C75",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "1h"
          },
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#E5C07B",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": false,
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "cache_duration": "15m"
          },
          "template": "{{ .HEAD }} ",
          "foreground": "#F3C267",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "style": "full"
          },
          "template": "{{ .Path }}",
          "foreground": "#61AFEF",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {},
          "template": " !",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {},
          "template": " Î»",
          "foreground": "#E06C75",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
