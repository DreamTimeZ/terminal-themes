{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {},
          "template": "{{ if gt .Code 0 }}✗ {{ .Code }} {{ end }}",
          "foreground": "#E06C75",
          "type": "status",
          "style": "plain"
        },
        {
          "properties": {
            "threshold": 2000,
            "style": "round"
          },
          "template": "{{ .FormattedMs }} ",
          "foreground": "#b8ff75",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "24h",
            "mapped_shell_names": {
              "bash": "Bash ",
              "cmd": "Cmd ",
              "powershell": "",
              "pwsh": ""
            }
          },
          "template": "{{ .Name }}",
          "foreground": "#E06C75",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "24h"
          },
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#E5C07B",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "branch_max_length": 20,
            "fetch_stash_count": false,
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "cache_duration": "30m"
          },
          "template": "{{ .HEAD }} ",
          "foreground": "#F3C267",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "style": "agnoster_short",
            "max_depth": 3
          },
          "template": "{{ .Path }} ",
          "foreground": "#61AFEF",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {},
          "template": "!",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {},
          "template": "λ",
          "foreground": "#E06C75",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true,
  "transient_prompt": {
    "foreground": "#E06C75",
    "template": "λ "
  }
}
